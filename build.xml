<project name="rbiodb" default="all">

	<!-- ALL -->
	<target name="all">
		<ant dir="src" inheritAll="false"/>
	</target>

	<!-- DOC -->
	<target name="doc"/>

	<!-- TEST -->
	<target name="test" depends="all">
		<ant dir="test" inheritAll="false"/>
	</target>

	<!-- CLEAN -->
	<target name="clean">
		<ant dir="test" target="clean" inheritAll="false"/>
	</target>

</project>
